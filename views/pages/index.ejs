    <!--Navbar/Header Ends-->


    <!--Carousel Begins-->
    <div id="carousel-slides" class="carousel slide carousel-fade" data-bs-ride="carousel">
        <div id="carousel-cover">

        </div>
        <div id="intro-container">
            <div id="introCard" class="card text-center" style="width: 18rem;">
                <div id="introCard-body" class="card-body">
                    <h5 class="card-title">Got no idea what exercises to do?</h5>
                    <div id="icon-container" class="d-flex justify-content-center justify-content-around">
                        <img src="/resources/images/Biceps.jpg" class="bd-placeholder-img rounded-circle" width="40"
                            height="40" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: 140x140"
                            preserveAspectRatio="xMidYMid slice" focusable="false">
                        <title>Biceps</title>
                        <rect width="100%" height="100%" fill="#777"></rect><text x="50%" y="50%" fill="#777"
                            dy=".3em"></text>
                        </img>
                        <img src="/resources/images/Back.png" class="bd-placeholder-img rounded-circle" width="40"
                            height="40" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: 140x140"
                            preserveAspectRatio="xMidYMid slice" focusable="false">
                        <title>Back</title>
                        <rect width="100%" height="100%" fill="#777"></rect><text x="50%" y="50%" fill="#777"
                            dy=".3em"></text>
                        </img>
                        <img src="/resources/images/front.png" class="bd-placeholder-img rounded-circle" width="40"
                            height="40" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: 140x140"
                            preserveAspectRatio="xMidYMid slice" focusable="false">
                        <title>Front</title>
                        <rect width="100%" height="100%" fill="#777"></rect><text x="50%" y="50%" fill="#777"
                            dy=".3em"></text>
                        </img>
                        <img src="/resources/images/legs.jpg" class="bd-placeholder-img rounded-circle" width="40" height="40"
                            xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: 140x140"
                            preserveAspectRatio="xMidYMid slice" focusable="false">
                        <title>Legs</title>
                        <rect width="100%" height="100%" fill="#777"></rect><text x="50%" y="50%" fill="#777"
                            dy=".3em"></text>
                        </img>
                    </div>
                    <br>
                    <p class="card-text">With just the simple touch of a button, a daily plan with different exercises
                        will
                        be generated to fit your
                        needs in terms of training.</p><br>
                        <script>

                            function scrollDown(){
                            window.scrollBy(0, window.innerHeight); // May need to be -1 to go down
                        }

                        </script>
                        <!--ondrop="scrollDown()"-->
                    <a href="/getexercises" id="generatorBtn" class="btn btn-info" role="button">Generate Plan</a>
                </div>
            </div>
        </div>

        <div id="carousel-body" class="carousel-inner">
            <div class="carousel-item active">
                <div id="carousel-img" class="d-block w-100"></div>
            </div>
            <div class="carousel-item">
                <div id="carousel-img2" class="d-block w-100"></div>
            </div>
            <div class="carousel-item">
                <div id="carousel-img3" class="d-block w-100"></div>
            </div>
        </div>
    </div>

    <!--Carousel Ends-->

    <!-- Cards Start -->

    <div id="mainContainer" class="album py-5 bg-light">
        <div class="container">

            <div
                class="d-flex justify-content-center justify-content-around row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
                
                <% for( let i = 0; i < exercises.length; i++ ) { %>
                <div class="cards-body card" style="width: 18rem;">
                    <div class="card-body">
                        <h3 class="card-title fs-4"><%= exercises[i].exerciseName %></h3>
                        <h6 class="card-subtitle mb-2 text-muted"> <%= exercises[i].area %></h6>
                        <hr>
                        <ul>
                            <li><b>Strength:</b> <%= exerciseQuantity[0] %> sets; <%= exerciseQuantity[1] %>
                                amount
                                of
                                repetitions</li>
                            <li><b>Growth:</b> <%= exerciseQuantity[2] %> sets; <%= exerciseQuantity[3] %>
                                amount of
                                repetitions
                            </li>
                            <li><b>Endurance:</b> <%= exerciseQuantity[4] %> sets;
                                <%= exerciseQuantity[5] %>
                                amount of
                                repetitions</li>
                        </ul>
                    </div>
                </div>
                <% } %>

            </div>
        </div>
        <br>
        <div class="container py-5">
            <p class="note card-subtitle mb-3 text-muted text-center">* Click the orange button saying "Generate Plan" to create a new set of cards with exercises.</p>
            <p class="note card-subtitle mb-2 text-muted text-center">* Note that the cards exercises gives a suggestions of reps and
                sets.
                More information can be found at the
                FAQ page.</p>
        </div>
        
    </div>

